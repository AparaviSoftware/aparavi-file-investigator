{
	"name": "aparavi-file-investigator",
	"version": "1.0.0",
	"description": "An LLM powered chat assistant to investigate large complex datasets.",
	"contributors": [
		{
			"name": "Joshua D. Phillips",
			"email": "josh.phillips@aparavi.com",
			"url": "https://github.com/joshuadarron"
		}
	],
	"private": true,
	"scripts": {
		"start": "pnpm build && concurrently -n \"backend,frontend\" -c \"blue,green\" \"pnpm --filter ./app start\" \"pnpm --filter ./client preview\"",
		"dev": "concurrently -n \"backend,frontend\" -c \"blue,green\" \"pnpm --filter ./app dev\" \"pnpm --filter ./client dev\"",
		"build": "pnpm --filter ./app build && pnpm --filter ./client build",
		"install:all": "pnpm install --filter ./app --filter ./client",
		"test": "pnpm --filter ./app test && pnpm --filter ./client test",
		"test:app": "pnpm --filter ./app test",
		"test:client": "pnpm --filter ./client test",
		"test:watch": "concurrently -n \"app,client\" -c \"blue,green\" \"pnpm --filter ./app test:watch\" \"pnpm --filter ./client test\"",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"eslint-plugin-import": "^2.32.0",
		"lucide-react": "^0.555.0",
		"moment": "^2.30.1"
	}
}
