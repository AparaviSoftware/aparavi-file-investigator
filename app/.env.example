# Server Configuration
PORT=3001
NODE_ENV=development

# Backend Type: 'express' or 'lambda'
# If 'lambda', the Express server will not start
BACKEND_TYPE=express

# Frontend Configuration
FRONTEND_URL=http://localhost:3000

# Webhook Configuration
WEBHOOK_BASE_URL=https://your-aparavi-instance.com/api/webhook/endpoint
WEBHOOK_API_KEY=your-api-key-here
WEBHOOK_AUTHORIZATION_KEY=your-authorization-key-here
WEBHOOK_TOKEN=your-webhook-token-here

# Security (optional)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info