# Backend Type: 'express' or 'lambda'
VITE_BACKEND_TYPE=express

# Express Backend URL (used when VITE_BACKEND_TYPE=express)
VITE_API_URL=http://localhost:3001

# Lambda Backend URL (used when VITE_BACKEND_TYPE=lambda)
# This should be your Lambda Function URL or API Gateway endpoint
# Example: https://abcd1234.lambda-url.us-east-1.on.aws
# Example: https://api-id.execute-api.us-east-1.amazonaws.com/prod
VITE_LAMBDA_URL=
