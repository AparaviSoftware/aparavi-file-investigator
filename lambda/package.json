{
	"name": "aparavi-chat-lambda",
	"version": "1.0.0",
	"description": "AWS Lambda function for Aparavi chat webhook integration",
	"main": "dist/handler.js",
	"scripts": {
		"build": "tsc",
		"clean": "rimraf dist function.zip",
		"package": "npm run build && bestzip function.zip dist node_modules",
		"type-check": "tsc --noEmit",
		"lint": "eslint . --ext .ts"
	},
	"keywords": [
		"aws-lambda",
		"webhook",
		"aparavi",
		"chat",
		"typescript"
	],
	"author": "Aparavi",
	"license": "MIT",
	"contributors": [
		{
			"name": "Aparavi Development Team"
		}
	],
	"dependencies": {
		"axios": "^1.6.7"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.138",
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"bestzip": "^2.2.1",
		"eslint": "^8.56.0",
		"rimraf": "^6.1.2",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
